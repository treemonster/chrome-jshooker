{

    "manifest_version": 2,
    "name": "jshooker",
    "description": "browser jshooker",
    "version": "1.0.0",
    "permissions": [
      "<all_urls>", "webRequest", "webRequestBlocking"
    ],
    "background": {
      "scripts": ["chrome.js"]
    },
    "browser_action":{
      "default_icon":"icon.png"
    }
}
