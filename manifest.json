{

    "manifest_version": 2,
    "name": "jshooker",
    "description": "browser jshooker",
    "version": "0.4.1",
    "permissions": [
      "<all_urls>", "webRequest", "webRequestBlocking"
    ],
    "background": {
      "scripts": ["cwg.js"]
    },
    "browser_action":{
      "default_icon":"icon.png",
      "default_popup":"cwg.html"
    }
}
